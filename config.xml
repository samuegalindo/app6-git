<?xml version="1.0" encoding="utf-8"?>

<widget xmlns = "thhp://www.w3.org/ns/widgets"
xmlns:gap = "http://phonegap.com/ns/1.0"
id = "com.phonegap.app6"
version = "1.0.0">

<name>app36 Deslizar hacia la izquierda y a la derecha </name>

<decription>
Aplicaión que realiza el swipe o deslizamiento en el dispositivi
</decription>

<author href="https://build.phonegsp.com" email="cb16.galindi.rodriguez.esteban@gmail.com">
Esteban Samuel Galindo Rodríguez
</author>

<!--Personaliza la aplicación y plataforms-->
<preference name="orientation"	value="default"/>
<!--<preference name="orientation"	value="portarit"/>
<preference name="orientation" value="landscape"/>-->
<preference name="target-device"	value="universsl"/>
<preference name="fullscreen"	value="true"/>
<preference name="android-installLocation"	value="auto"/>
<preference name="permissions"	value="INTERNET"/>


<!--Plugins-->
<!--Cone plugins-->
<plugin name="cordova-plugin-device" spec="~1.1.4"/>
<plugin name="cordova-plugin-vibration" spec="~2.1.3"/>
<plugin name="cordova-plugin-dialogs" spec="~1.3.1"/>

<!--<plugin name="cordova-plugin-geolocation" source="npm"/>-->

<!--Para accesar a paginas en internet-->
<allow-intent href="https://*/*"/>
<acces origin="https://*/*"/>
<allow-intent href="http://*/*"/>
<acces origin="http://*/*"/>
<plugin name="corodva-plugin-whitelist" source="npm"/>

<!--Colocar icono app-->

<icon src="res/android/mdpi.png"/>
</widget>

